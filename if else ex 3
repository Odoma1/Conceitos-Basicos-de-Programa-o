#include<stdio.h>
main()
{
	float n1, n2,n3,n4, m;
	printf("Informe a primeira nota: ");
	scanf("%f", &n1);
	printf("Informe a segunda nota: ");
	scanf("%f", &n2);
	printf("Informe a terceira nota: ");
	scanf("%f", &n3);
	printf("Informe a quarta nota: ");
	scanf("%f", &n4);
	m=(n1+n2+n3+n4)/4;
	printf("media:%.2f", m);
	if(m>=6)
	{
	printf("\n Aprovado");
	}
		else
	{
	printf("\n Reprovado");
	}	
}
