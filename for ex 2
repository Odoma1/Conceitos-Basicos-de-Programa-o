#include <stdio.h>
#include <math.h>

int main()
{
    int n, i;
	float media;
    for(i = 1; i <= 4; i++)
    {
        printf("Informe os numeros: ");
        scanf("%i", &n);         
 		media = (media+n);
		 media= media/4; 
    }
	      
        printf("A media desses numeros Ã© %.2f\n", media); 
    return 0;
}
